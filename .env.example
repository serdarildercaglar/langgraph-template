# Environment
ENVIRONMENT=development  # development, staging, production

# vLLM Settings
VLLM_BASE_URL=http://localhost:8000/v1
VLLM_API_KEY=EMPTY
VLLM_MODEL_NAME=your-model-name
VLLM_TEMPERATURE=0.7
VLLM_MAX_TOKENS=4096
VLLM_TIMEOUT=60.0
VLLM_MAX_RETRIES=3

# Embedding Settings
EMBEDDING_BASE_URL=http://localhost:8001/v1
EMBEDDING_API_KEY=EMPTY
EMBEDDING_MODEL_NAME=bge-large-en-v1.5
EMBEDDING_BATCH_SIZE=32
EMBEDDING_MAX_CONCURRENT=5
EMBEDDING_DIMENSION=1024

# Milvus Settings
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_USER=
MILVUS_PASSWORD=
MILVUS_DATABASE=default
MILVUS_COLLECTION_NAME=documents
MILVUS_BATCH_SIZE=100
MILVUS_MAX_CONCURRENT=3
MILVUS_TIMEOUT=30.0

# Langfuse Settings
LANGFUSE_ENABLED=true
LANGFUSE_PUBLIC_KEY=pk-lf-your-public-key
LANGFUSE_SECRET_KEY=sk-lf-your-secret-key
LANGFUSE_HOST=https://cloud.langfuse.com

# Checkpoint Settings
CHECKPOINT_BACKEND=sqlite  # sqlite, postgres, memory
CHECKPOINT_SQLITE_PATH=./checkpoints.db
CHECKPOINT_POSTGRES_URI=

# API Settings
API_TITLE=LangGraph Agentic API
API_VERSION=0.1.0
API_HOST=0.0.0.0
API_PORT=8080
API_DEBUG=false
API_CORS_ORIGINS=["*"]
